<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义多条件筛选下拉view-->
    <declare-styleable name="DropMenuView">
        <attr name="menuDividerColor" format="color" />
        <attr name="tabDividerColor" format="color" />
        <attr name="tabTextColorSelected" format="color" />
        <attr name="tabTextColorUnselected" format="color" />
        <attr name="menuBackgroundColor" format="color" />
        <attr name="maskColor" format="color" />
        <attr name="tabTextSize" format="dimension" />
        <attr name="tabSelectedIcon" format="reference" />
        <attr name="tabUnselectedIcon" format="reference" />
        <attr name="menuHeightPercent" format="float" />
        <attr name="tabHeight" format="dimension" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="indicatorSelectedColor" format="color" />
        <attr name="indicatorUnSelectedColor" format="color" />

    </declare-styleable>

    <declare-styleable name="TabView">
        <attr name="bottomLineColor" format="color" />
    </declare-styleable>

</resources>